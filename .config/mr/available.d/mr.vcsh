[$HOME/.config/vcsh/repo.d/config-mr.git]
checkout = 
  vcsh clone https://github.com/Shen-Jing/config-mr.git config-mr
  cd config-mr
  git remote add upstream git@github.com:Shen-Jing/config-mr.git
update = vcsh config-mr pull 
push = vcsh config-mr push
status = vcsh config-mr status
gc = vcsh config-mr gc
